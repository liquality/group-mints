{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "es5",
      "es6"
   ],
   "target": "es5",
   "module": "commonjs",
   "moduleResolution": "node",
   "sourceMap": true
  }
}
